    "build:prod":"vue-tsc --noEmit && vite build --mode prod"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.7.7",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.7.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "npm-run-all2": "^6.2.0",
    "typescript": "~5.4.0",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
